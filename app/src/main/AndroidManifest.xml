<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.monster.webmon">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".indexCompany">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".calc" />
        <activity android:name=".cart" />
        <activity android:name=".companyInfo" />
        <activity android:name=".about" />
        <activity android:name=".contact" />
        <activity android:name=".info" />
        <activity android:name=".register" />
        <activity android:name=".toast" />
        <activity android:name=".contactDetails" />
        <activity android:name=".splash"></activity>
    </application>
    <uses-permission android:name="android.permission.CALL_PHONE" />

</manifest>